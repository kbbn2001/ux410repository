{
  "_version": "1.37.0",
  "sap.app": {
    "id": "student00.sap.training.dynamicpage",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.0",
      "toolsId": "82215de8-13b2-4748-8588-ba79709f4984"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/UX_TRAVEL_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.96.16",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "student00.sap.training.dynamicpage.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "student00.sap.training.dynamicpage.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "idFlexLayout",
        "clearControlAggregation": true
      },
      "routes": [
        {
          "name": "Overview",
          "pattern": "",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "Overview"
          ]
        },
        {
          "name": "Carrier",
          "pattern": "carriers/{carrierId}",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "Carrier"
          ]
        }
      ],
      "targets": {
        "Overview": {
          "viewType": "XML",
          "viewName": "Overview",
          "viewLevel": 1
        },
        "NotFound": {
          "viewType": "XML",
          "viewName": "NotFound",
          "controlAggregation": "midColumnPages"
        },
        "Carrier": {
          "viewType": "XML",
          "viewName": "Carrier",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "student00.sap.training.dynamicpage.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}